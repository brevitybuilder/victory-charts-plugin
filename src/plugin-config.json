{
  "id": "victory-chart",
  "name": "Victory Chart Plugin",
  "description": "Victory Chart Plugin",
  "private": true,
  "publicProps": [],
  "privateProps": [],
  "blocks": [
    {
      "name": "Chart",
      "entrypoint": "./blocks/chart/index.tsx",
      "acceptsChildren": false,
      "category": "Charts",
      "props": [
        {
          "label": "Config",
          "name": "config",
          "type": { "kind": "victory-chart|Config" },
          "default": null,
          "placeholder": "...",
          "help": "The config prop"
        }
      ],
      "events": [],
      "defaultStyles": {
        "": {
          "width": "100%"
        }
      },
      "sources": [],
      "actions": []
    }
  ],
  "schemas": [
    {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "id": "Config",
      "type": "object",
      "properties": {
        "chartType": {
          "type": "string"
        },
        "columns": {
          "type": "array",
          "items": {
            "$ref": "victory-chart|Column"
          }
        },
        "rows": {
          "type": "array",
          "items": {
            "$ref": "victory-chart|Row"
          }
        },
        "filters": {
          "type": "array",
          "items": {
            "$ref": "victory-chart|Filter"
          }
        },
        "option1": {
          "type": "string"
        },
        "option2": {
          "type": "string"
        },
        "option3": {
          "type": "string"
        }
      },
      "required": ["chartType", "columns", "rows"]
    },
    {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "id": "Column",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "options": {
          "type": "array",
          "items": {
            "$ref": "victory-chart|Option"
          }
        }
      },
      "required": ["id", "title", "type"]
    },
    {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "id": "Row",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "complete": {
          "type": "boolean"
        },
        "cells": {
          "type": "array",
          "items": {
            "$ref": "victory-chart|Cell"
          }
        }
      },
      "required": ["id", "boolean"]
    },
    {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "id": "Filter",
      "type": "object",
      "properties": {
        "columnId": {
          "type": "string"
        },
        "operator": {
          "type": "string"
        },
        "textValue": {
          "type": "string"
        },
        "numberValue": {
          "type": "number"
        },
        "dateValue": {
          "type": "string"
        },
        "selectValue": {
          "type": "string"
        },
        "userValue": {
          "type": "string"
        },
        "multiSelectValue": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["columnId", "operator"]
    },
    {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "id": "Cell",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "rowId": {
          "type": "string"
        },
        "columnId": {
          "type": "string"
        },
        "textValue": {
          "type": "string"
        },
        "numberValue": {
          "type": "number"
        },
        "dateValue": {
          "type": "string"
        },
        "selectValue": {
          "type": "string"
        },
        "userValue": {
          "type": "string"
        },
        "multiSelectValue": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["id", "rowId", "columnId"]
    },
    {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "id": "Option",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      },
      "required": ["id", "title"]
    }
  ],
  "serverActions": []
}
