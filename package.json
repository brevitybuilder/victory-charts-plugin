{
  "name": "brevity-plugin-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "publish-plugin": "brevity plugin publish patch -c ./src/plugin-config.json",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,css,html}'",
    "check": "tsc -p tsconfig.json --noEmit",
    "lint": "oxlint --fix",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@brevity-builder/react": "^1.2.170",
    "clsx": "^2.1.1",
    "react": "rc",
    "react-dom": "rc",
    "react-is": "rc",
    "recharts": "2.13.0",
    "vitest": "^2.1.3",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@brevity-builder/cli": "^1.2.155",
    "@builder.io/partytown": "^0.10.2",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "esbuild-css-modules-plugin": "^3.1.2",
    "oxlint": "^0.10.2",
    "plop": "^4.0.1",
    "storybook": "^8.3.6",
    "typescript": "^5.6.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "use-sync-external-store": "^1.2.2",
    "vite": "^5.4.10"
  },
  "pnpm": {
    "overrides": {
      "react": "$react",
      "react-dom": "$react-dom",
      "react-is": "$react-is"
    },
    "patchedDependencies": {
      "recharts": "patches/recharts.patch"
    }
  }
}
