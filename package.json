{
  "name": "brevity-plugin-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "publish-plugin": "brevity plugin publish patch -c ./src/plugin-config.json",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,css,html}'",
    "check": "tsc -p tsconfig.json --noEmit",
    "lint": "oxlint --fix",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@brevity-builder/react": "^1.2.207",
    "clsx": "^2.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-is": "19.0.0",
    "recharts": "^2.14.1",
    "vitest": "^2.1.8",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@brevity-builder/cli": "^1.2.185",
    "@builder.io/partytown": "^0.10.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild-css-modules-plugin": "^3.1.2",
    "oxlint": "^0.14.1",
    "plop": "^4.0.1",
    "storybook": "^8.4.7",
    "typescript": "^5.7.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "use-sync-external-store": "^1.4.0",
    "vite": "^6.0.3"
  },
  "pnpm": {
    "overrides": {
      "react-is": "$react"
    },
    "patchedDependencies": {
      "recharts": "patches/recharts.patch"
    }
  }
}
